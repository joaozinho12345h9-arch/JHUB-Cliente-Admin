-- O código abaixo é a representação ofuscada de toda a lógica do JHUB
local _0x99_data = [[
    local p = game:GetService("Players"); local lp = p.LocalPlayer; local rs = game:GetService("RunService")
    local cg = game:GetService("CoreGui"); local uis = game:GetService("UserInputService")
    local sg = Instance.new("ScreenGui", cg:FindFirstChild("RobloxGui") or lp.PlayerGui); sg.Name = "JHUB_SEC"
    local function _d(o) local dg, di, ds, sp; o.InputBegan:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 or i.UserInputType == Enum.UserInputType.Touch then dg = true; ds = i.Position; sp = o.Position end end) uis.InputChanged:Connect(function(i) if dg and (i.UserInputType == Enum.UserInputType.MouseMovement or i.UserInputType == Enum.UserInputType.Touch) then local dl = i.Position - ds; o.Position = UDim2.new(sp.X.Scale, sp.X.Offset + dl.X, sp.Y.Scale, sp.Y.Offset + dl.Y) end end) uis.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 or i.UserInputType == Enum.UserInputType.Touch then dg = false end end) end
    local m = Instance.new("Frame", sg); m.Size = UDim2.new(0, 380, 0, 350); m.Position = UDim2.new(0.5, -190, 0.4, 0); m.BackgroundColor3 = Color3.fromRGB(10,10,10); m.Visible = false; Instance.new("UICorner", m); _d(m)
    local s = Instance.new("UIStroke", m); s.Thickness = 3; rs.RenderStepped:Connect(function() s.Color = Color3.fromHSV(tick()%5/5, 1, 1) end)
    local b = Instance.new("TextButton", sg); b.Size = UDim2.new(0, 200, 0, 60); b.Position = UDim2.new(0, 20, 0.5, 0); b.Text = "J  H  U  B"; b.TextColor3 = Color3.new(1,1,1); b.Font = 3; b.TextSize = 25; b.BackgroundColor3 = Color3.fromRGB(15,15,15); Instance.new("UICorner", b); _d(b); b.MouseButton1Click:Connect(function() m.Visible = not m.Visible end)
    local st = Instance.new("UIStroke", b); st.Thickness = 2; rs.RenderStepped:Connect(function() st.Color = Color3.fromHSV(tick()%5/5, 1, 1) end)
    local tx = Instance.new("TextBox", m); tx.Size = UDim2.new(0, 340, 0, 40); tx.Position = UDim2.new(0.5, -170, 0, 15); tx.PlaceholderText = "NICK DO ALVO"; tx.BackgroundColor3 = Color3.fromRGB(25,25,25); tx.TextColor3 = Color3.new(1,1,1); Instance.new("UICorner", tx)
    local sc = Instance.new("ScrollingFrame", m); sc.Size = UDim2.new(1, -20, 1, -85); sc.Position = UDim2.new(0, 10, 0, 70); sc.BackgroundTransparency = 1; sc.CanvasSize = UDim2.new(0,0,6,0); Instance.new("UIListLayout", sc).Padding = UDim.new(0, 5)
    local function _ex(c, t) local v = nil; for _, plr in pairs(p:GetPlayers()) do if plr.Name:lower():sub(1,#t) == t:lower() then v = plr; break end end if not v then v = lp end
    if (v.Name == "jotinha12hr2" or v.Name == "amiguinhodoheito") and (c=="kill" or c=="kick" or c=="fling") and lp.Name ~= v.Name then return end
    local ch = v.Character; local h = ch and ch:FindFirstChildOfClass("Humanoid"); local r = ch and ch:FindFirstChild("HumanoidRootPart")
    if c == "kill" then r.CFrame = CFrame.new(0,-1000,0); h.Health = 0 elseif c == "fling" then h.PlatformStand = true; local bv = Instance.new("BodyVelocity", r); bv.MaxForce = Vector3.new(9e9,9e9,9e9); bv.Velocity = Vector3.new(0,5000,0); task.wait(0.2); bv:Destroy() elseif c == "kick" then v:Kick("JHUB VIP") elseif c == "bring" then r.CFrame = lp.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-3) end end
    local bt = {"Kill", "Fling", "Kick", "Void", "Bring", "Jail", "Unjail", "Speed Boost", "God Mode", "Invisible", "View", "Unview", "BTools", "Freeze", "Unfreeze", "Explode", "Verify", "Jump", "Sit", "Infect", "Blind", "Spin", "Unspin", "Full Bright", "Anti-AFK", "Control", "Uncontrol", "Lag Server", "Jump Boost"}
    for _, n in pairs(bt) do local btn = Instance.new("TextButton", sc); btn.Size = UDim2.new(1,-5,0,40); btn.Text = n:upper(); btn.BackgroundColor3 = Color3.fromRGB(30,30,30); btn.TextColor3 = Color3.new(1,1,1); btn.Font = 3; Instance.new("UICorner", btn); btn.MouseButton1Click:Connect(function() _ex(n:lower(), tx.Text) end) end
    local function tg(pl, tx) local function at() local h = pl.Character and pl.Character:WaitForChild("Head", 15) if h then local bg = Instance.new("BillboardGui", h); bg.Name = "ST"; bg.Size = UDim2.new(0,200,0,50); bg.ExtentsOffset = Vector3.new(0,3,0); bg.AlwaysOnTop = true; local tl = Instance.new("TextLabel", bg); tl.Size = UDim2.new(1,0,1,0); tl.BackgroundTransparency = 1; tl.Text = "[ "..tx.." ]"; tl.Font = 3; tl.TextSize = 25; tl.TextStrokeTransparency = 0; rs.RenderStepped:Connect(function() if tl.Parent then tl.TextColor3 = Color3.fromHSV(tick()%5/5, 1, 1) end end) end end at(); pl.CharacterAdded:Connect(at) end
    for _, pl in pairs(p:GetPlayers()) do if pl.Name == "jotinha12hr2" then tg(pl, "DONO JHUB") elseif pl.Name == "amiguinhodoheito" then tg(pl, "SUB-DONO") end end
]]

-- Execução do motor protegido
local _0x_init = loadstring(_0x99_data)
