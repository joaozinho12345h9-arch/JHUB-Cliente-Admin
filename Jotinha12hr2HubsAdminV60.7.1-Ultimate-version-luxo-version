        end
    end
    tag(); plr.CharacterAdded:Connect(tag)
end

local function Refresh()
    for _, v in pairs(p:GetPlayers()) do
        if v.Name == DONO then applyTag(v, "DONO JHUB", true)
        elseif v.Name == SUB then applyTag(v, "SUB-DONO", true)
        elseif v == lp then applyTag(v, "ADMIN JHUB", false) end
    end
end
Refresh(); p.PlayerAdded:Connect(function() task.wait(2); Refresh() end)

local function _exe(cmd)
    local v = nil; for _,pl in pairs(p:GetPlayers()) do if pl.Name:lower():sub(1,#TargetNick)==TargetNick:lower() or pl.DisplayName:lower():sub(1,#TargetNick)==TargetNick:lower() then v=pl;break end end
    if not v and (cmd~="uncontrol" and cmd~="ann" and cmd~="bright") then return end
    if v and (v.Name == DONO or v.Name == SUB) and (lp.Name ~= DONO and lp.Name ~= SUB) then return end

    local hrp = v and v.Character and v.Character:FindFirstChild("HumanoidRootPart")
    local hum = v and v.Character and v.Character:FindFirstChildOfClass("Humanoid")

    if cmd == "kill" then hrp.CFrame = CFrame.new(0,-1000,0); hum.Health = 0
    elseif cmd == "kick" then v:Kick("[JHUB LUXURY] Sua licen√ßa de player expirou.")
    elseif cmd == "jail" then
        if workspace:FindFirstChild("Jail_"..v.Name) then workspace["Jail_"..v.Name]:Destroy() end
        local m=Instance.new("Model",workspace); m.Name="Jail_"..v.Name
        local function pt(sz,cf) local b=Instance.new("Part",m); b.Size,b.Anchored,b.Material,b.Color,b.CFrame=sz,true,"Neon",Color3.new(1,0,0),hrp.CFrame*cf end
        pt(Vector3.new(12,1,12),CFrame.new(0,-3.5,0)); pt(Vector3.new(1,8,12),CFrame.new(5.5,0,0))
        spawn(function() while workspace:FindFirstChild("Jail_"..v.Name) do if hrp then hrp.CFrame = m:GetModelCFrame() end; task.wait(0.1) end end)
    elseif cmd == "control" then MyOriginalChar=lp.Character; lp.Character=v.Character; workspace.CurrentCamera.CameraSubject=hum
    elseif cmd == "uncontrol" then lp.Character=MyOriginalChar; workspace.CurrentCamera.CameraSubject=lp.Character:FindFirstChildOfClass("Humanoid")
    elseif cmd == "freeze" then hrp.Anchored = true
    elseif cmd == "unfreeze" then hrp.Anchored = false
    elseif cmd == "bring" then hrp.CFrame = lp.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-3)
    elseif cmd == "goto" then lp.Character.HumanoidRootPart.CFrame = hrp.CFrame * CFrame.new(0,0,3)
    end
end

