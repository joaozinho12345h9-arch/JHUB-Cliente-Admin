local TargetNick = ""; local MyOriginalChar = lp.Character; local DONO = "jotinha12hr2"; local SUB = "amiguinhodoheito"

local function applyTag(plr, txt, isRGB)
    local function tag()
        local char = (plr == lp) and MyOriginalChar or plr.Character
        local h = char and char:WaitForChild("Head", 15)
        if h then 
            if h:FindFirstChild("JH_TAG") then h.JH_TAG:Destroy() end
